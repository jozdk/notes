{
  "name": "notes",
  "version": "1.0.0",
  "description": "",
  "main": "server/app.js",
  "scripts": {
    "start": "cross-env REQUEST_LOG_FILE=log.txt REQUEST_LOG_FORMAT=common DEBUG=notes:* node app",
    "dev": "cross-env REQUEST_LOG_FILE=log.txt REQUEST_LOG_FORMAT=common DEBUG=notes:* nodemon app",
    "fs-start": "cross-env DEBUG=notes:* PORT=3000 NOTES_MODEL=fs node app.js",
    "fs-server2": "cross-env PORT=3001 NOTES_MODEL=fs node app.js",
    "level-start": "cross-env DEBUG=notes:* PORT=3000 NOTES_MODEL=level node app.js",
    "sqlite3-setup": "sqlite3 notes.sqlite3 --init models/schema-sqlite3.sql",
    "sqlite3-start": "cross-env DEBUG=notes:* NOTES_MODEL=sqlite3 node app.js",
    "sqlite3-server2": "cross-env NOTES_MODEL=sqlite3 PORT=3001 node app.js",
    "sequelize-start": "cross-env DEBUG=notes:* SEQUELIZE_CONNECT=server/models/sequelize-sqlite.yaml NOTES_MODEL=sequelize node server/app.js & npm run dev:build-client",
    "sequelize-server2": "cross-env SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize PORT=3001 node app.js",
    "sequelize-pg-start": "cross-env DEBUG=notes:* SEQUELIZE_CONNECT=models/sequelize-pg.yaml NOTES_MODEL=sequelize node app.js",
    "build:css": "postcss ./main.css -o ./public/stylesheets/styles.css --watch --verbose",
    "prod:css": "NODE_ENV=production postcss ./main.css -o ./public/stylesheets/styles.css",
    "dev:build-client": "webpack watch --config ./webpack.config.js --env MODE=development"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-handlebars": "^6.0.4",
    "express-session": "^1.17.2",
    "fs-extra": "^10.1.0",
    "js-yaml": "^4.1.0",
    "level": "^8.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "rotating-file-stream": "^3.0.3",
    "sequelize": "^6.19.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.5.0",
    "sqlite3": "^5.0.4",
    "superagent": "^7.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.24",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
